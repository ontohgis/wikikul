<https://onto.kul.pl/ontowikikul/>
	a owl:Ontology ;
	rdfs:comment "Ontologia dla modelu danych wiki.kul.pl" ;
	.

ontowikikul:person
	a owl:Class ;
	rdfs:label
		"Osoba"@pl ,
		"Person"@de ,
		"Person"@en ,
		"Personne"@fr ,
		"Pessoa"@pt ,
		"Πρόσωπο"@el ,
		"Личность"@ru ,
		"人物"@zh
		;
	.

ontowikikul:parish_latin
	a owl:Class ;
	rdfs:label
		"Pfarre"@de ,
